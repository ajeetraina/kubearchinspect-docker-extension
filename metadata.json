{
  "icon": "kubearchinspect.svg",
  "vm": {
    "image": "${DESKTOP_PLUGIN_IMAGE}",
    "composefile": "docker-compose.yaml"
  },
  "ui": {
    "dashboard-tab": {
      "title": "KubeArchInspect",
      "root": "/ui",
      "src": "index.html"
    }
  },
  "host": {
    "binaries": [
      {
        "darwin": [
          {
            "path": "/darwin/kubectl"
          }
        ],
        "linux": [
          {
            "path": "/linux/kubectl"
          }
        ],
        "windows": [
          {
            "path": "/windows/kubectl.exe"
          }
        ]
      }
    ]
  }
}
